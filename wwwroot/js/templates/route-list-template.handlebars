                    </svg>
                    <svg viewBox="0 0 24 24">
                        <path fill="{{#if_eq note null}} #ddd {{else}} #666 {{/if_eq}}" d="M14,10H19.5L14,4.5V10M5,3H15L21,9V19A2,2 0 0,1 19,21H5C3.89,21 3,20.1 3,19V5C3,3.89 3.89,3 5,3M5,12V14H19V12H5M5,16V18H14V16H5Z" />
                    </svg>
                    <svg viewBox="0 0 24 24">
                        <path fill="{{#if hasImage}} #666 {{else}} #ddd {{/if}}" d="M8.5,13.5L11,16.5L14.5,12L19,18H5M21,19V5C21,3.89 20.1,3 19,3H5A2,2 0 0,0 3,5V19A2,2 0 0,0 5,21H19A2,2 0 0,0 21,19Z" />
                    </svg>
                </div>
                <div class="clear"></div>
            </div>
            <div class="route-card-bottom">
                <span class="route-author">Made by: {{author}}</span>
                <span class="route-date">{{date}}</span>
                <div class="clear"></div>
            </div>
        </div>
    </a>
    {{/each}}
</div>